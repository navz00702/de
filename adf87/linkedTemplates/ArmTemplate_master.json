{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf87"
		},
		"LinkedService1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkedService1'"
		},
		"SqlServer_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_LS'",
			"defaultValue": "Integrated Security=False;Data Source=@{linkedService().SVRname};Initial Catalog=@{linkedService().DBname};User ID=@{linkedService().username};Password=@{linkedService().pwd}"
		},
		"ls_git_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_git'"
		},
		"LinkedService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls94.dfs.core.windows.net/"
		},
		"ls_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls94.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf87_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedService1_accountKey": {
						"value": "[parameters('LinkedService1_accountKey')]"
					},
					"SqlServer_LS_connectionString": {
						"value": "[parameters('SqlServer_LS_connectionString')]"
					},
					"ls_git_accountKey": {
						"value": "[parameters('ls_git_accountKey')]"
					},
					"LinkedService1_properties_typeProperties_url": {
						"value": "[parameters('LinkedService1_properties_typeProperties_url')]"
					},
					"ls_git_properties_typeProperties_url": {
						"value": "[parameters('ls_git_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}